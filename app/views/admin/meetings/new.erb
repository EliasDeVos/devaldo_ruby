<div class="container-fluid">
    <h1>Add Meeting</h1>
    <div>
        <%= form_for :meeting, url:  admin_meetings_path do |f| %>
            <div class="form-group">
                <%= f.label :name %>
                <%= f.text_field :name, class: 'form-control' %>
            </div>
            <div class="form-group">
                <%= f.label :start_time %>
                <%= f.text_field :start_time, class: 'form-control' %>
            </div>
            <div class="form-group">
                <%= f.label :location %>
                <%= f.text_field :location, class: 'form-control' %>
            </div>
                <%= f.submit 'Save meeting', class: 'btn btn-primary' %>
                <%= link_to 'Back', admin_meetings_path, class: "btn btn-primary" %>
            <p>
            </p>
        <% end %>
    </div>
</div>
