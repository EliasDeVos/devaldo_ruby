<%= form_for :game, url:  admin_games_path do |f| %>
    <p>
        <%= f.label :homeTeam %><br>
        <%= f.text_field :homeTeam %><br>
    </p>
    <p>
        <%= f.label :score %><br>
        <%= f.text_field :score %><br>
    </p>
    <p>
        <%= f.label :awayTeam %><br>
        <%= f.text_field :awayTeam %><br>
    </p>
    <p>
        <%= f.label :description %><br>
        <%= f.text_area :description %><br>
    </p>
        <%= f.submit %>
    <p>
    </p>
<% end %>

<%= link_to 'Back', admin_games_path %>
